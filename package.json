{
  "name": "@gargdev/cache",
  "version": "0.0.1",
  "description": "A universal caching solution with a global storage adapter, Fetch/Axios wrappers, decorators, and middleware support â€” for both client and server.",
  "author": {
    "name": "Aryan Garg",
    "url": "https://www.npmjs.com/~gargdev"
  },
  "homepage": "https://github.com/Aryan-Garg-dev/webtools-cache",
  "bugs": {
    "url": "https://github.com/Aryan-Garg-dev/webtools-cache/issues"
  },
  "repository": {
