{
  "name": "@gargdev/cache",
  "version": "1.0.6",
  "description": "Lightweight Redis caching decorators and wrappers for Node and Bun",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "private": "false",
  "keywords": ["ioredis", "bun"],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "bun --watch examples/http-server.ts",
    "build": "bunx tsc -p tsconfig.build.json && bun build src/index.ts --outdir dist"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "fast-json-stable-stringify": "^2.1.0",
    "ioredis": "^5.6.0",
    "superjson": "^2.2.2"
  }
}
